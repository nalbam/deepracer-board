# Environment Variables
# Copy this file to .env.local and update the values

# Next.js
NEXTAUTH_URL=https://dracer.io
NEXTAUTH_SECRET=your-nextauth-secret-here

# AWS Cognito
AUTH_COGNITO_CLIENT_ID=your-cognito-client-id
AUTH_COGNITO_CLIENT_SECRET=your-cognito-client-secret
AUTH_COGNITO_ISSUER=https://cognito-idp.ap-northeast-2.amazonaws.com/your-user-pool-id

# AWS Credentials
AUTH_AWS_REGION=ap-northeast-2
AUTH_AWS_ACCESS_KEY_ID=your-access-key-id
AUTH_AWS_SECRET_ACCESS_KEY=your-secret-access-key

# DynamoDB Tables
NEXT_DYNAMODB_LEAGUES_TABLE=deepracer-board-leagues
NEXT_DYNAMODB_RACERS_TABLE=deepracer-board-racers
